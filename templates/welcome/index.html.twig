        * {
            margin: 0;
            padding: 0;
        }

        .home-image {
            width: 40% !important;
            border-radius: 5px;
            margin: 2em;
        }

        .candy-image {
            width: 25% !important;
            border-radius: 5px;
        }

        h2 {
            font-size: 4em;
        }

        h1 {
            font-size: 5em;
        }

        .headline-text {
            font-family: 'Noto Sans', sans-serif;
        }

        .browse-btn {
            font-size: 2em;
            font-family: 'Noto Sans', sans-serif;
            border: 2px solid #292b2c;
            font-weight: bold;
        }

        .row {
            --bs-gutter-x: 0 !important;
        }
        .card {
            border : 0px solid #292b2c !important;
