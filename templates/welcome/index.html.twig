    <div class="container">
        <div class="card border-dark mt-5">
            <div class="row">
                <div class="col-lg-6">
                    <div class="card-body text-dark headline-text">
                        <h2>Milktoasts</h2>
                        <h1>Art webshop</h1>
                        <button type="button" class="btn btn-outline-dark browse-btn mt-3">Browse</button>
                    </div>
                </div>
                <div class="col-lg-6">
                    <img src="{{ asset('build/images/dexter.jpg') }}" class="home-image" alt="dexter">
                    <img src="{{ asset('build/images/candy.jpg') }}" class="candy-image" alt="candy">
